{
  // Cloudflare Workers configuration
  "name": "rhule-aid",
  "compatibility_date": "2024-01-01",
  "compatibility_flags": ["nodejs_compat"],
  
  // Main Worker script
  "main": "./src/index.ts",
  
  // Static assets configuration
  "assets": {
    "directory": "./public",
    "binding": "ASSETS"
  },
  
  // D1 Database binding
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "rhule-aid-db",
      "database_id": "dbed4dc5-b282-40b0-a196-d90ea7cc74e0"
    }
  ],
  
  // KV Namespace bindings
  "kv_namespaces": [
    {
      "binding": "SCHEDULE_CACHE",
      "id": "b6ba8e644c1447699ef2dfbd88e0527e"
    },
    {
      "binding": "ROSTER_CACHE", 
      "id": "2d472c3e9eee4576952c9496876a8e5f"
    },
    {
      "binding": "NEWS_CACHE",
      "id": "44fa2bcea61241cf8af0cd346d53505d"
    },
    {
      "binding": "WEATHER_CACHE",
      "id": "cc8345020c464041b7ce0af0ad277dba"
    },
    {
      "binding": "CFB_SCHEDULE_CACHE",
      "id": "9f74b018fd3e4283b2a8bb9711046b84"
    },
    {
      "binding": "RANKINGS_CACHE",
      "id": "1a8a0e3162444dca81c77f79aae677e3"
    },
    {
      "binding": "STANDINGS_CACHE",
      "id": "aa2a13f482454648b6c1cbe2cdd91cbc"
    }
  ],
  
  // Environment variables
  "vars": {
    "ENVIRONMENT": "production"
  },
  
  // Custom domains configuration
  "routes": [
    { "pattern": "rhule-aid.com/*", "zone_name": "rhule-aid.com" },
    { "pattern": "www.rhule-aid.com/*", "zone_name": "rhule-aid.com" }
  ],
  
  // Scheduled cron triggers
  "triggers": {
    "crons": ["0 1 * * 1"]
  },
  
  // Observability configuration
  "observability": {
    "enabled": true
  }
}