{
  "name": "nebraska-football-site",
  "version": "1.0.0",
  "description": "Nebraska Cornhuskers Football Fan Website with Cloudflare Pages Functions",
  "main": "data-fetcher.js",
  "scripts": {
    "start": "node data-fetcher.js",
    "update-data": "node data-fetcher.js", 
    "schedule": "node -e \"const scraper = require('./data-fetcher'); scraper.getLatestSchedule().then(console.log)\"",
    "roster": "node -e \"const scraper = require('./data-fetcher'); scraper.getLatestRoster().then(console.log)\"",
    "build:functions": "wrangler pages functions build --outdir=./dist/worker/",
    "dev": "npm run build:functions && wrangler dev",
    "deploy": "./deploy.sh",
    "deploy:windows": "deploy.bat",
    "build": "npm run build:functions && tsc --noEmit",
    "test": "echo \"No tests specified\" && exit 0"
  },  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "cross-fetch": "^4.1.0",
    "node-fetch": "^3.3.0",
    "puppeteer": "^22.15.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "@types/node": "^18.19.0",
    "nodemon": "^3.0.0",
    "typescript": "^5.3.0",
    "wrangler": "^4.26.1"
  },
  "keywords": [
    "nebraska",
    "football",
    "cornhuskers",
    "cloudflare",
    "pages",
    "web-scraping",
    "sports-data"
  ],
  "author": "Nebraska Football Fan",
  "license": "MIT"
}
